template(name="adminLayout")
	if isLoading
		.ui.active.inverted.page.dimmer
			.ui.text.loader #{isLoading}
	+uploadProgress
	+confirmView
	+promptView
	.ui.fluid.container
		.ui.yellow.right.corner.label
			p(style="transform: rotate(45deg);margin-top:10px;margin-left:20px;") Beta

		+Template.dynamic template=menu
		.ui.grid
			.three.wide.column
				+Template.dynamic template=left
			.thirteen.wide.column
				+Template.dynamic template=main
